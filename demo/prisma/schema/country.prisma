model Country {
    countryCode               String @id @map("country_code")
    currencyCode              String @map("currency_code")
    countryNameEn             String @map("country_name_en")
    countryNameLocal          String @map("country_name_local")
    currencyNameEn            String @map("currency_name_en")
    tinType                   String @map("tin_type")
    tinName                   String @map("tin_name")
    officialLanguageCode      String @map("official_language_code")
    officialLanguageNameEn    String @map("official_language_name_en")
    officialLanguageNameLocal String @map("official_language_name_local")
    countryCallingCode        String @map("country_calling_code")
    region                    String @map("region")
    flag                      String @map("flag")

    users User[]

    @@index([countryCode(sort: Asc)], type: BTree, name: "countries:country_code:btree_asc")
    @@index([countryCode(sort: Desc)], type: BTree, name: "countries:country_code:btree_desc")
    @@index([countryCode(ops: raw("gin_trgm_ops"))], type: Gin, name: "countries:country_code:gin")
    @@index([currencyCode(sort: Asc)], type: BTree, name: "countries:currency_code:btree_asc")
    @@index([currencyCode(sort: Desc)], type: BTree, name: "countries:currency_code:btree_desc")
    @@index([currencyCode(ops: raw("gin_trgm_ops"))], type: Gin, name: "countries:currency_code:gin")
    @@index([countryNameEn(sort: Asc)], type: BTree, name: "countries:country_name_en:btree_asc")
    @@index([countryNameEn(sort: Desc)], type: BTree, name: "countries:country_name_en:btree_desc")
    @@index([countryNameEn(ops: raw("gin_trgm_ops"))], type: Gin, name: "countries:country_name_en:gin")
    @@index([currencyNameEn(sort: Asc)], type: BTree, name: "countries:currency_name_en:asc_btree")
    @@index([currencyNameEn(sort: Desc)], type: BTree, name: "countries:currency_name_en:desc_btree")
    @@index([currencyNameEn(ops: raw("gin_trgm_ops"))], type: Gin, name: "countries:currency_name_en:gin")
    @@index([officialLanguageNameEn(sort: Asc)], type: BTree, name: "countries:official_language_name_en:asc_btree")
    @@index([officialLanguageNameEn(sort: Desc)], type: BTree, name: "countries:official_language_name_en:desc_btree")
    @@index([officialLanguageNameEn(ops: raw("gin_trgm_ops"))], type: Gin, name: "countries:official_language_name_en:gin")
    @@map("countries")
}
